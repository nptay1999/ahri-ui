  "version": "0.0.0",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/west-ui.umd.cjs",
  "module": "./dist/west-ui.js",
  "exports": {
    ".": {
      "import": "./dist/west-ui.js",
      "require": "./dist/west-ui.umd.cjs"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "license": "MIT",
  "scripts": {
    "dev": "storybook dev -p 6006",
    "start": "vite",
